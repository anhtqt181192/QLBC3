<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width,initial-scale=1" />
    <meta http-equiv='cache-control' content='no-cache' />
    <meta http-equiv='expires' content='0' />
    <meta http-equiv='pragma' content='no-cache' />
    <title>QLHK</title>
    <!-- Bootstrap core CSS -->
    <link rel="icon" type="image/png" href="icons/32.png" />
    <link type="text/css" rel="stylesheet" href="/css/Plugin/font-awesome-4.6.3/css/font-awesome.min.css" />
    <link rel="stylesheet" href="/css/Plugin/bootstrap-4.0.0-alpha.4/dist/css/bootstrap.css" />
    <link rel="stylesheet" href="/css/Plugin/style/AnimationBoder.css" />
    <link rel="stylesheet" href="/css/Plugin/style/hover-min.css" />
    <!-- Custom styles for this template -->
    <link rel="stylesheet" href="/css/jquery-ui.css" />
    <link rel="stylesheet" href="/css/main.css" />
    @RenderSection("css", required: false)
</head>
<body>
    <div id="right-context-menu">
        <div class="right-context-menu" onclick="addRowBefore(this)"><span style="color: #5cb85c"><i class="fa fa-arrow-up"></i></span> thêm dòng trên</div>
        <div class="right-context-menu" onclick="addRowAfter(this)"><span style="color: #5cb85c"><i class="fa fa-arrow-down"></i></span> thêm dòng dưới</div>
        <div class="right-context-menu" onclick="hideRow(this)"><span style="color: #ff0000"><i class="fa fa-remove"></i></span> xóa tạm thời</div>
    </div>
    <nav class="navbar navbar-full navbar-light" style="border-bottom: 1px solid #ccc;padding:0px;">
        @Html.Partial("_ViewMenu.cshtml")
    </nav>
    <div class="card-block active tab-hide" id="active-tab1" style="height: 100%; width: 100%;">
        @RenderBody()
    </div>
    <div class="mini-layout container-fluid" id="layout-print-table"></div>
    <footer>
        Developer By  <a href="mailto:tuananh181192@gmail.com" style="color: #43a7fd"> Trần Quốc Tuấn Anh </a>®
    </footer>
    <script type="text/javascript" src="/js/jquery.min.js"></script>
    <script type="text/javascript" src="/Css/Plugin/bootstrap-4.0.0-alpha.4/js/src/dropdown.js"></script>
    <script type="text/javascript" src="/Css/Plugin/bootstrap-4.0.0-alpha.4/js/src/Util.js"></script>
    <script type="text/javascript" src="/Css/Plugin/bootstrap-4.0.0-alpha.4/js/src/Modal.js"></script>
    <script type="text/javascript" src="/js/jquery-ui.js"></script>
    <script type="text/javascript" src="/js/main.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
